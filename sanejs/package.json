{
  "name": "@lacorazon/sanejs",
  "version": "1.0.0",
  "description": "Trying out SaneJS https://www.sanejs.dev/",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "tsc",
    "start": "node dist/index.js",
    "watch1": "fuser -k 3000/tcp ; tsc  && node dist/index.js",
    "watch": "chokidar ./src/**/*.ts ./routes/**/*.html --initial -c \"npm run watch1 \"",
    "build": "tsc",
    "test": "node --inspect-brk dist/index.js"
  },
