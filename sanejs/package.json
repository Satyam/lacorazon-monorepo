    "livereload": "^0.10.2",
    "marked": "^16.1.2",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "node-html-parser-hyperscript": "^5.4.3",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7"
  },
